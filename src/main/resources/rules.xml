<?xml version="1.0" encoding="UTF-8"?>
<rules>
    <rule>
        <key>mutant.survived</key>
        <name>Survived Mutants</name>
        <description>
            <![CDATA[<h2>Survived mutant</h2>
	       <p>A mutant of a class has not been killed although appearing to be covered by a test.</p>
	       <p>
	       For more information check out the <a href=\"https://stryker-mutator.io/docs/stryker-net/Introduction\">Stryker documentation</a>
	       </p>
           ]]>
        </description>
        <type>BUG</type>
        <tag>stryker</tag>
        <tag>test</tag>
        <tag>test-quality</tag>
        <tag>survived</tag>
        <tag>mutation</tag>
    </rule>
    <rule>
        <key>mutant.lurking</key>
        <name>Lurking Mutants</name>
        <description>
            <![CDATA[<h2>Uncovered mutant</h2>
	       <p>A mutant of a class has not been covered by a test at all.</p>
	       <p>
	       For more information check out the <a href=\"https://stryker-mutator.io/docs/stryker-net/Introduction\">Stryker documentation</a>
	       </p>
           ]]>
        </description>
        <type>BUG</type>
        <tag>stryker</tag>
        <tag>test</tag>
        <tag>test-quality</tag>
        <tag>uncovered</tag>
        <tag>mutation</tag>
    </rule>
</rules>

